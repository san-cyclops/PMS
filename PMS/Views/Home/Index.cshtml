@model PMS.Models.SessionKey
@inject IJsonHelper Json;
<script type="text/javascript">
    var SessionKey = @Html.Raw(@Json.Serialize(Model));
</script>
@{
    ViewData["Title"] = "Home Page";
}
<div ng-app="APIModule">
    <div ng-controller="APIController">
        <div id="wrapper">
            <!--BEGIN SIDEBAR MENU-->

            @Html.Partial("_MainMenu")


            <div id="page-wrapper">
                <!--BEGIN TITLE & BREADCRUMB PAGE-->
                <!--SUB TITLE -- SANJEEWA-->
                <!--END TITLE & BREADCRUMB PAGE-->
                <!--BEGIN CONTENT-->
                <div class="page-content">
                    <div id="tab-general">
                        <div class="row mbl">
                            <div class="col-lg-12">
                                <div class="col-lg-3">
                                </div>
                                <div class="col-lg-6">
                                    <div ng-show="feedback">

                                        <div class="panel panel-yellow">
                                            <div class="panel-heading">
                                                Feed Back
                                            </div>
                                            <div class="panel-body pan">
                                                <div class="form-horizontal">
                                                    <div class="form-body pal">
                                                        <div class="form-group col-md-12">
                                                           Please take a moment to fill out this Feedback Form
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="inputPassword" class="col-md-3 control-label">
                                                                Feedback
                                                            </label>
                                                            <div class="col-md-9">
                                                                <div class="input-icon right">
                                                                    <textarea rows="5" class="form-control"></textarea>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        
                                                    </div>
                                                    <div class="form-actions pal">
                                                        <div class="form-group mbn">
                                                            <div class="col-md-offset-3 col-md-6">
                                                                <button class="btn btn-primary" ng-click="getfeedback()">
                                                                    Submit Feedback
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                    </div>
                                </div>
                                <div class="col-lg-3">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--END CONTENT-->
                <!--BEGIN FOOTER-->
                <div id="footer">
                    <div class="copyright">

                    </div>
                </div>
                <!--END FOOTER-->
            </div>

        </div>
    </div>
</div>
<script src="https://code.angularjs.org/1.5.8/angular.js"></script>
<script src="~/js/protractor.js"></script>
<script src="~/js/app_Index.js"></script>
